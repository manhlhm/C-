using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Manh // chuong trinh hien 10 so theo chieu doc
    {
        static void Main(string[] args)
        {
            int x, y;
            for (x =10; x < 10; x++, Console.WriteLine(" ")) ;
            for (y = 0; y < 10; y++, Console.WriteLine("{0}", y)) ;
               
       
            Console.ReadLine();
       
        }      
    }
    }

